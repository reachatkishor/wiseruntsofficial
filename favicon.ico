<svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 32 32">
  <!-- Cosmic background -->
  <rect width="32" height="32" fill="#000000"/>
  
  <!-- Stars background -->
  <circle cx="4" cy="4" r="0.5" fill="#ffffff" opacity="0.8"/>
  <circle cx="28" cy="6" r="0.3" fill="#ffffff" opacity="0.6"/>
  <circle cx="26" cy="26" r="0.4" fill="#ffffff" opacity="0.7"/>
  <circle cx="6" cy="28" r="0.3" fill="#ffffff" opacity="0.5"/>
  <circle cx="24" cy="12" r="0.3" fill="#ffffff" opacity="0.8"/>
  <circle cx="8" cy="16" r="0.2" fill="#ffffff" opacity="0.6"/>
  <circle cx="20" cy="28" r="0.3" fill="#ffffff" opacity="0.4"/>
  <circle cx="12" cy="6" r="0.2" fill="#ffffff" opacity="0.7"/>
  
  <!-- Central "W" with glow -->
  <defs>
    <filter id="glow">
      <feGaussianBlur stdDeviation="1.5" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <text x="16" y="22" font-family="Arial, sans-serif" font-size="16" font-weight="bold" 
        text-anchor="middle" fill="#ffffff" filter="url(#glow)">W</text>
  
  <!-- Subtle cosmic ring -->
  <circle cx="16" cy="16" r="12" fill="none" stroke="rgba(255,255,255,0.1)" stroke-width="0.5"/>
</svg>
