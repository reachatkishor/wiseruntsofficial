<svg xmlns="http://www.w3.org/2000/svg" width="180" height="180" viewBox="0 0 180 180">
  <!-- Cosmic gradient background -->
  <defs>
    <radialGradient id="cosmicBg" cx="50%" cy="50%">
      <stop offset="0%" stop-color="#1a1a2e"/>
      <stop offset="50%" stop-color="#0f0f23"/>
      <stop offset="100%" stop-color="#000000"/>
    </radialGradient>
    <filter id="starGlow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    <filter id="textGlow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <rect width="180" height="180" fill="url(#cosmicBg)" rx="20"/>
  
  <!-- Starfield -->
  <circle cx="30" cy="25" r="1.5" fill="#ffffff" filter="url(#starGlow)"/>
  <circle cx="150" cy="35" r="1" fill="#ffffff" opacity="0.8"/>
  <circle cx="165" cy="60" r="1.2" fill="#ffffff" filter="url(#starGlow)"/>
  <circle cx="25" cy="80" r="0.8" fill="#ffffff" opacity="0.6"/>
  <circle cx="160" cy="120" r="1" fill="#ffffff" opacity="0.7"/>
  <circle cx="40" cy="140" r="1.5" fill="#ffffff" filter="url(#starGlow)"/>
  <circle cx="170" cy="155" r="0.8" fill="#ffffff" opacity="0.8"/>
  <circle cx="15" cy="160" r="1" fill="#ffffff" opacity="0.5"/>
  <circle cx="50" cy="170" r="1.2" fill="#ffffff" filter="url(#starGlow)"/>
  <circle cx="130" cy="25" r="0.8" fill="#ffffff" opacity="0.6"/>
  <circle cx="110" cy="165" r="1" fill="#ffffff" opacity="0.7"/>
  <circle cx="20" cy="50" r="0.8" fill="#ffffff" opacity="0.5"/>
  
  <!-- Galaxy spiral hint -->
  <circle cx="90" cy="90" r="60" fill="none" stroke="rgba(255,255,255,0.05)" stroke-width="1"/>
  <circle cx="90" cy="90" r="80" fill="none" stroke="rgba(255,255,255,0.03)" stroke-width="0.5"/>
  
  <!-- Main "W" with cosmic styling -->
  <text x="90" y="115" font-family="Arial, sans-serif" font-size="80" font-weight="300" 
        text-anchor="middle" fill="#ffffff" filter="url(#textGlow)">W</text>
  
  <!-- Subtle brand text -->
  <text x="90" y="140" font-family="Arial, sans-serif" font-size="12" font-weight="300" 
        text-anchor="middle" fill="#cccccc" opacity="0.8">WISERUNTS</text>
</svg>
